                              window.location.hash = data.event.target.hash;
                          }, 10); // Small delay to prevent Barba.js from getting stuck
                      }
                      return;
                  },
              },
          ],
      });
  };
  const replaceContent = (next, selector) => {
      // Create a temporary DOM from next.html
      const parser = document.createElement('div');
      parser.innerHTML = next.html;
      // Query the new menu from the parsed HTML
      const newMenu = parser.querySelector(selector);
      const currentMenu = document.querySelector(selector);
      // Replace current menu with the new one
      if (newMenu && currentMenu) {
          currentMenu.replaceWith(newMenu);
      }
  };

