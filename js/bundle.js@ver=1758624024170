      if (!!key && Object.getPrototypeOf(key) === Object.prototype) {
        cache[id] = _objectSpread2$1(_objectSpread2$1({}, data), key);
      }
    } else if (key !== undefined) {
      data[key] = value;
    }

    return value;
  }
  function get(owner, key) {
