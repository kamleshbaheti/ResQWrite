  const staggerElements = document.querySelectorAll(".c-loader__stagger");
  const loader = document.querySelector(".c-loader__spinner-wrap");
  module_default$2.data("pageLoader", () => ({
      init() {
          this.enter();
          Z.hooks.leave(this.leave.bind(this));
          Z.hooks.enter(this.enter.bind(this));
      },
      enter() {
          gsapWithCSS.fromTo(loader, {
              opacity: 1,
              scale: 1,
          }, {
              opacity: 0,
              // scale: 0.5
          });
          gsapWithCSS.fromTo(staggerElements, { x: "0%" }, {
              x: "100%",
              stagger: -0.1,
              onComplete: () => {
                  this.$root.classList.add("hidden");
              },
          });
      },
      leave() {
          this.$root.classList.remove("hidden");
          return new Promise((resolve) => {
              gsapWithCSS.fromTo(loader, {
                  opacity: 0,
                  // scale: 0.5
              }, {
                  opacity: 1,
                  scale: 1,
              });
              gsapWithCSS.fromTo(staggerElements, { x: "-100%" }, {
                  x: "0%",
                  stagger: 0.1,
                  onComplete: () => {
                      resolve(true);
                  },
              });
          });
      },
  }));

