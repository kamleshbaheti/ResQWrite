              y: "100%",
          }, {
              // x: "0%",
              y: "0%",
              ease: "sine.inOut",
              duration: 0.5,
              stagger: 0.1,
          });
      },
      close() {
          gsapWithCSS.fromTo(this.$root.querySelector(textSelector), {
              opacity: 1
          }, {
              opacity: 0,
          });
          gsapWithCSS.fromTo(this.$root.querySelectorAll(staggerSelector), {
              // x: "0%",
              y: "0%",
          }, {
              // x: "100%",
              y: "100%",
              ease: 'sine.inOut',
              stagger: -0.1,
              duration: 0.5,
              onComplete: () => {
                  this.isOpen = false;
              },
          });
      },
  }));

