              isSplit = _data$get.isSplit,
              html = _data$get.html,
              cssWidth = _data$get.cssWidth,
              cssHeight = _data$get.cssHeight;

          if (isSplit) {
            element.innerHTML = html;
            element.style.width = cssWidth || '';
            element.style.height = cssHeight || '';
            remove(element);
          }
        });
      }
      /**
