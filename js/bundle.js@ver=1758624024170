  module_default$2.data("scrollStatements", () => ({
      tl: null,
      textElements: null,
      init() {
          if (document.querySelector('.wp-admin'))
              return;
          this.$root.style.minHeight = '600vh';
          const tl = gsapWithCSS.timeline({
              onUpdate: this.updateProgress.bind(this),
              scrollTrigger: {
                  trigger: this.$root,
                  toggleActions: "play",
                  start: "top top",
                  end: "bottom bottom",
                  pin: this.$refs.container,
                  scrub: 1,
              },
          });
          const textElements = this.$root.querySelectorAll('.c-scroll-statements__item-text');
          this.textElements = textElements;
          Array.from(textElements).forEach(element => {
              const closestItem = element.closest('.c-scroll-statements__item');
              tl.from(element, {
                  height: 0,
                  ease: "power4.out",
                  onStart: () => {
                      closestItem.classList.add('is-active');
                  },
                  onReverseComplete: () => {
                      closestItem.classList.remove('is-active');
                  },
              }).to(element, {
                  height: 0,
                  ease: "power4.inOut",
                  onComplete: () => {
                      closestItem.classList.remove('is-active');
                  },
                  onReverseComplete: () => {
                      closestItem.classList.add('is-active');
                  },
              });
          });
          this.tl = tl;
      },
      jumpTo(index) {
          if (!this.textElements.length) {
              console.log('scroll-statements: no text elements to scroll to');
          }
          const targetProgress = (index + 0.5) / (this.textElements.length);
          const scrollTrigger = this.tl.scrollTrigger;
          if (scrollTrigger) {
              const scrollY = scrollTrigger.start + (scrollTrigger.end - scrollTrigger.start) * targetProgress;
              // window.scrollTo(0, scrollY);
              gsapWithCSS.set(window, {
                  scrollTo: scrollY,
              });
          }
      },
      updateProgress() {
          if (this.$refs.progress) {
              this.$refs.progress.style.width = this.tl.progress() * 100 + '%';
          }
      },
      destroy() {
          // console.log('scroll-statements destroy')
      }
  }));

  module_default$2.data("cardSlider", () => ({
      swiper: null,
      scrollTimeline: null,
      init() {
          const swiperElement = this.$refs.swiper;
          // animate on scroll
          if (!document.querySelector(".wp-admin")) {
              const cards = Array.from(this.$root.querySelectorAll(".c-card-slider__card ")).slice(0, 5); // take only the first 5
              this.scrollTimeline = gsapWithCSS
                  .timeline({
                  scrollTrigger: {
                      trigger: this.$root,
                      // start: 'top 50%',
                      start: "-50% bottom",
                      end: "50% 50%",
                      scrub: 1.5,
                  },
              })
                  .from(cards, {
                  x: "50%",
                  y: "10%",
                  rotation: "10deg",
                  ease: "power1.out",
                  stagger: 0.1,
              });
          }
          const timeline = this.scrollTimeline;
          this.swiper = new Swiper(swiperElement, {
              modules: [Navigation, Pagination, EffectCreative],
              grabCursor: true,
              speed: 400,
              spaceBetween: 16,
              slidesPerView: 1.25,
              breakpoints: {
                  600: {
                      slidesPerView: 2,
                  },
                  900: {
                      slidesPerView: 3,
                  },
                  1280: {
                      slidesPerView: 4,
                      spaceBetween: 24,
                  },
                  1600: {
                      slidesPerView: 4,
                      spaceBetween: 32,
                  },
              },
              watchSlidesProgress: true,
              on: {
                  setTranslate(swiper, transition) {
                      // don't do rotations on small screens
                      if (window.innerWidth < 800) {
                          return;
                      }
                      swiper.slides.forEach((slide, index) => {
                          if (isNaN(slide.progress)) {
                              console.warn('cardSlider: progress not working...');
                          }
                          // fallback to negative index as slide progress is NaN on init
                          const slideProgressReal = !isNaN(slide.progress)
                              ? slide.progress
                              : index * -1;
                          const slideProgressShifted = slideProgressReal + 2; // + 2 to shift the slides
                          const translateY = Math.abs(slideProgressShifted * slideProgressShifted * 3) + "%";
                          const rotate = slideProgressShifted * -4 + 'deg';
                          slide.style.transform = `translateY(${translateY}) rotate(${rotate})`;
                      });
                  },
                  setTransition(swiper, duration) {
                      swiper.slides.forEach((slideEl) => {
                          slideEl.style.transition = `${duration}ms`;
                      });
                  },
                  init(swiper) {
                      setTimeout(() => {
                          swiper.setTranslate(swiper);
                      }, 50);
                  },
                  afterInit: (swiper) => {
                      var _a;
                      (_a = swiper.navigation) === null || _a === void 0 ? void 0 : _a.update();
                  },
                  beforeSlideChangeStart: () => {
                      console.log(timeline, timeline.scrollTrigger);
                      ScrollTrigger$1.update();
                      const st = timeline.scrollTrigger;
                      const progress = timeline.progress();
                      console.log('progress', progress);
                      if (st) {
                          st.update();
                          st.disable(true);
                      } // unpin & stop scroll control
                      timeline.progress(progress);
                      timeline.play();
                  }
              },
              // centeredSlides: true,
              // initialSlide: 1,
              navigation: {
                  nextEl: ".swiper-button-next",
                  prevEl: ".swiper-button-prev",
              },
              pagination: {
                  el: ".swiper-pagination",
                  type: "bullets",
              },
          });
      },
      destroy() {
          this.swiper.destroy();
      },
  }));
  module_default$2.data("cardSliderCard", () => ({
      open: false,
      init() { }
  }));

